[remote "heketi"]
	url = git@github.com:heketi/heketi.git
    fetch = +refs/heads/*:refs/remotes/heketi/*
[remote "origin"]
	url = git@github.com:huangyingw/heketi.git 
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = heketi
    branch = release/10
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[gdif]
    ignore = *.go.sh
    ignore = install.sh
    ignore = run.sh
    ignore = setup.sh
    ignore = topology.json
